{
  "name": "minecraft-launcher",
  "version": "1.0.0",
  "description": "A custom Minecraft launcher built with Electron.",
  "main": "src/main/index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win portable"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "discord-rpc": "^4.0.1",
    "electron-log": "^5.2.4",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.3.9",
    "is-elevated": "^3.0.0",
    "minecraft-auth": "^1.0.0",
    "minecraft-launcher-core": "^3.18.2",
    "minecraft-protocol": "^1.44.0",
    "mojang-api": "^0.0.2",
    "msmc": "^5.0.5",
    "node-mc-api": "^1.2.2"
  },
  "devDependencies": {
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "nodemon": "^3.1.9"
  },
  "build": {
    "appId": "com.projectvir.launcher",
    "productName": "ProjectLauncher",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        "portable"
      ],
      "icon": "assets/project.ico",
      "executableName": "ProjectLauncher",
      "requestedExecutionLevel": "requireAdministrator"
    }
  },
  "author": "Sobini_",
  "license": "MIT"
}
